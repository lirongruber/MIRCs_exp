
X=[forSVM{1}(:,1:8) ; forSVM{2}(:,1:8)];
Y=zeros(size(X,1),1);
Y(size(forSVM{1},1)+1:end,1)=1;
XX=X(nansum(X,2)~=0,:);
YY=Y(nansum(X,2)~=0);

n=0;
label={};
for i=1:10:160
    n=n+1;
    XX=XX([1:i-1 i+1:end],:);
    YY=YY([1:i-1 i+1:end]);
    x_test=XX(i,:);
    y_label=YY(i);
    SVMModel = fitcsvm(XX,YY,'KernelFunction','rbf','Standardize',true,'ClassNames',{'0','1'});
    [label(n),score] = predict(SVMModel,x_test);
    sucsses(n)=strcmp(label{n},num2str(y_label));
end